{
	"paths": {
		"/{{ endpointSignature }}": {
			"{{ httpVerb }}": {
				"tags": [
					"{{ endpointName }}"
				],
				"summary": "{{ pathVerb }} {{ endpointName }}",
				"operationId": "{{ operationId }}",
				"parameters": [{
					"in": "path",
					"name": "{{ pathParam }}",
					"required": true,
					"schema": {
						"type": "string",
							"pattern": "^[0-9a-fA-F]{24}$"
						}
					}
				]
			}
		}
	}
}
